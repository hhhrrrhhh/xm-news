{"ast":null,"code":"export default {\n  data() {\n    return {\n      provinceId: null,\n      provinceName: '',\n      newsList: [],\n      pageSize: 50,\n      currentPage: 1,\n      totalPages: 1\n    };\n  },\n  computed: {\n    pagedNewsList() {\n      const start = (this.currentPage - 1) * this.pageSize;\n      const end = start + this.pageSize;\n      return this.newsList.slice(start, end);\n    }\n  },\n  mounted() {\n    this.provinceId = this.$route.query.provinceId;\n    this.loadProvinceNews();\n  },\n  methods: {\n    loadProvinceNews() {\n      this.$request.get('/provinces/selectById', {\n        params: {\n          provincesId: this.provinceId\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.provinceName = res.data.provincesName;\n          this.newsList = res.data.newsList;\n          this.totalPages = Math.ceil(this.newsList.length / this.pageSize);\n        } else {\n          this.$message.error(res.msg);\n        }\n      }).catch(error => {\n        console.error(\"Error loading province news:\", error);\n      });\n    },\n    changePage(page) {\n      if (page > 0 && page <= this.totalPages) {\n        this.currentPage = page;\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","provinceId","provinceName","newsList","pageSize","currentPage","totalPages","computed","pagedNewsList","start","end","slice","mounted","$route","query","loadProvinceNews","methods","$request","get","params","provincesId","then","res","code","provincesName","Math","ceil","length","$message","error","msg","catch","console","changePage","page"],"sources":["src/views/front/ProvinceNews.vue"],"sourcesContent":["<template>\r\n  <div class=\"news-list\">\r\n    <h1>{{ provinceName }} 新闻列表</h1>\r\n    <ul>\r\n      <li v-for=\"(item, index) in pagedNewsList\" :key=\"item.id\">\r\n        <span class=\"number\">{{ (currentPage - 1) * pageSize + index + 1 }}.</span>\r\n        <a @click=\"$router.push('/front/newsDetail?id=' + encodeURIComponent(item.id))\">{{ item.title }}</a>\r\n      </li>\r\n    </ul>\r\n    <div class=\"pagination\">\r\n      <button @click=\"changePage(currentPage - 1)\" :disabled=\"currentPage === 1\">上一页</button>\r\n      <button @click=\"changePage(currentPage + 1)\" :disabled=\"currentPage === totalPages\">下一页</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      provinceId: null,\r\n      provinceName: '',\r\n      newsList: [],\r\n      pageSize: 50,\r\n      currentPage: 1,\r\n      totalPages: 1,\r\n    };\r\n  },\r\n  computed: {\r\n    pagedNewsList() {\r\n      const start = (this.currentPage - 1) * this.pageSize;\r\n      const end = start + this.pageSize;\r\n      return this.newsList.slice(start, end);\r\n    }\r\n  },\r\n  mounted() {\r\n    this.provinceId = this.$route.query.provinceId;\r\n    this.loadProvinceNews();\r\n  },\r\n  methods: {\r\n    loadProvinceNews() {\r\n      this.$request.get('/provinces/selectById', {\r\n        params: { provincesId: this.provinceId }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.provinceName = res.data.provincesName;\r\n          this.newsList = res.data.newsList;\r\n          this.totalPages = Math.ceil(this.newsList.length / this.pageSize);\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      }).catch(error => {\r\n        console.error(\"Error loading province news:\", error);\r\n      });\r\n    },\r\n    changePage(page) {\r\n      if (page > 0 && page <= this.totalPages) {\r\n        this.currentPage = page;\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.news-list {\r\n  width: 80%;\r\n  margin: 20px auto;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.news-list h1 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.news-list ul {\r\n  list-style: none;\r\n  padding: 0;\r\n}\r\n\r\n.news-list ul li {\r\n  border-bottom: 1px solid #eee;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  display: flex;\r\n  align-items: center;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n\r\n.news-list ul li a {\r\n  color: #007bff;\r\n  text-decoration: none;\r\n  flex-grow: 1;\r\n  display: block;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n\r\n.news-list ul li a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n.news-list ul li .number {\r\n  font-weight: bold;\r\n  margin-right: 10px;\r\n  color: #555;\r\n  flex-shrink: 0;\r\n}\r\n\r\n.pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.pagination button {\r\n  padding: 10px 20px;\r\n  margin: 0 10px;\r\n  border: none;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n}\r\n\r\n.pagination button:disabled {\r\n  background-color: #ccc;\r\n  cursor: not-allowed;\r\n}\r\n</style>\r\n"],"mappings":"AAiBA;EACAA,KAAA;IACA;MACAC,UAAA;MACAC,YAAA;MACAC,QAAA;MACAC,QAAA;MACAC,WAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACAC,cAAA;MACA,MAAAC,KAAA,SAAAJ,WAAA,aAAAD,QAAA;MACA,MAAAM,GAAA,GAAAD,KAAA,QAAAL,QAAA;MACA,YAAAD,QAAA,CAAAQ,KAAA,CAAAF,KAAA,EAAAC,GAAA;IACA;EACA;EACAE,QAAA;IACA,KAAAX,UAAA,QAAAY,MAAA,CAAAC,KAAA,CAAAb,UAAA;IACA,KAAAc,gBAAA;EACA;EACAC,OAAA;IACAD,iBAAA;MACA,KAAAE,QAAA,CAAAC,GAAA;QACAC,MAAA;UAAAC,WAAA,OAAAnB;QAAA;MACA,GAAAoB,IAAA,CAAAC,GAAA;QACA,IAAAA,GAAA,CAAAC,IAAA;UACA,KAAArB,YAAA,GAAAoB,GAAA,CAAAtB,IAAA,CAAAwB,aAAA;UACA,KAAArB,QAAA,GAAAmB,GAAA,CAAAtB,IAAA,CAAAG,QAAA;UACA,KAAAG,UAAA,GAAAmB,IAAA,CAAAC,IAAA,MAAAvB,QAAA,CAAAwB,MAAA,QAAAvB,QAAA;QACA;UACA,KAAAwB,QAAA,CAAAC,KAAA,CAAAP,GAAA,CAAAQ,GAAA;QACA;MACA,GAAAC,KAAA,CAAAF,KAAA;QACAG,OAAA,CAAAH,KAAA,iCAAAA,KAAA;MACA;IACA;IACAI,WAAAC,IAAA;MACA,IAAAA,IAAA,QAAAA,IAAA,SAAA5B,UAAA;QACA,KAAAD,WAAA,GAAA6B,IAAA;MACA;IACA;EACA;AACA"},"metadata":{},"sourceType":"module","externalDependencies":[]}