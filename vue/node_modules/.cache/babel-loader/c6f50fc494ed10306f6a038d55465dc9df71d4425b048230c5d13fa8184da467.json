{"ast":null,"code":"export default {\n  data() {\n    return {\n      provinceId: null,\n      provinceName: '',\n      newsList: [],\n      pageSize: 15,\n      currentPage: 1,\n      totalPages: 1\n    };\n  },\n  mounted() {\n    this.provinceId = this.$route.query.provinceId;\n    this.loadProvinceInfo();\n    this.loadProvinceNews();\n  },\n  methods: {\n    loadProvinceInfo() {\n      this.$request.get('/provinces/selectAll').then(res => {\n        const province = res.data.find(p => p.provincesId == this.provinceId);\n        if (province) {\n          this.provinceName = province.provincesName;\n        } else {\n          this.$message.error('省份信息加载失败');\n        }\n      }).catch(error => {\n        console.error(\"Error loading provinces:\", error);\n      });\n    },\n    loadProvinceNews() {\n      this.$request.get('/news/selectPage', {\n        params: {\n          pageNum: this.currentPage,\n          pageSize: this.pageSize,\n          provincesId: this.provinceId\n        }\n      }).then(res => {\n        if (res.code === '200') {\n          this.newsList = res.data.list || [];\n          this.totalPages = Math.ceil(res.data.total / this.pageSize);\n        } else {\n          this.$message.error(res.msg);\n        }\n      }).catch(error => {\n        console.error(\"Error loading news:\", error);\n      });\n    },\n    changePage(page) {\n      if (page > 0 && page <= this.totalPages) {\n        this.currentPage = page;\n        this.loadProvinceNews();\n      }\n    }\n  }\n};","map":{"version":3,"names":["data","provinceId","provinceName","newsList","pageSize","currentPage","totalPages","mounted","$route","query","loadProvinceInfo","loadProvinceNews","methods","$request","get","then","res","province","find","p","provincesId","provincesName","$message","error","catch","console","params","pageNum","code","list","Math","ceil","total","msg","changePage","page"],"sources":["src/views/front/ProvinceNews.vue"],"sourcesContent":["<template>\r\n  <div class=\"news-list\">\r\n    <h1>{{ provinceName }} 新闻列表</h1>\r\n    <ul>\r\n      <li v-for=\"(item, index) in newsList\" :key=\"item.id\">\r\n        <span class=\"number\">{{ (currentPage - 1) * pageSize + index + 1 }}.</span>\r\n        <a @click=\"$router.push('/front/newsDetail?id=' + encodeURIComponent(item.id))\">{{ item.title }}</a>\r\n      </li>\r\n    </ul>\r\n    <div class=\"pagination\">\r\n      <button @click=\"changePage(currentPage - 1)\" :disabled=\"currentPage === 1\">上一页</button>\r\n      <button @click=\"changePage(currentPage + 1)\" :disabled=\"currentPage === totalPages\">下一页</button>\r\n    </div>\r\n  </div>\r\n</template>\r\n\r\n<script>\r\nexport default {\r\n  data() {\r\n    return {\r\n      provinceId: null,\r\n      provinceName: '',\r\n      newsList: [],\r\n      pageSize: 15,\r\n      currentPage: 1,\r\n      totalPages: 1,\r\n    };\r\n  },\r\n  mounted() {\r\n    this.provinceId = this.$route.query.provinceId;\r\n    this.loadProvinceInfo();\r\n    this.loadProvinceNews();\r\n  },\r\n  methods: {\r\n    loadProvinceInfo() {\r\n      this.$request.get('/provinces/selectAll').then(res => {\r\n        const province = res.data.find(p => p.provincesId == this.provinceId);\r\n        if (province) {\r\n          this.provinceName = province.provincesName;\r\n        } else {\r\n          this.$message.error('省份信息加载失败');\r\n        }\r\n      }).catch(error => {\r\n        console.error(\"Error loading provinces:\", error);\r\n      });\r\n    },\r\n    loadProvinceNews() {\r\n      this.$request.get('/news/selectPage', {\r\n        params: {\r\n          pageNum: this.currentPage,\r\n          pageSize: this.pageSize,\r\n          provincesId: this.provinceId,\r\n        }\r\n      }).then(res => {\r\n        if (res.code === '200') {\r\n          this.newsList = res.data.list || [];\r\n          this.totalPages = Math.ceil(res.data.total / this.pageSize);\r\n        } else {\r\n          this.$message.error(res.msg);\r\n        }\r\n      }).catch(error => {\r\n        console.error(\"Error loading news:\", error);\r\n      });\r\n    },\r\n    changePage(page) {\r\n      if (page > 0 && page <= this.totalPages) {\r\n        this.currentPage = page;\r\n        this.loadProvinceNews();\r\n      }\r\n    }\r\n  }\r\n}\r\n</script>\r\n\r\n<style scoped>\r\n.news-list {\r\n  width: 80%;\r\n  margin: 20px auto;\r\n  font-family: Arial, sans-serif;\r\n}\r\n\r\n.news-list h1 {\r\n  text-align: center;\r\n  margin-bottom: 20px;\r\n}\r\n\r\n.news-list ul {\r\n  list-style: none;\r\n  padding: 0;\r\n}\r\n\r\n.news-list ul li {\r\n  border-bottom: 1px solid #eee;\r\n  padding: 10px 0;\r\n  font-size: 16px;\r\n  display: flex;\r\n  align-items: center;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n\r\n.news-list ul li a {\r\n  color: #007bff;\r\n  text-decoration: none;\r\n  flex-grow: 1;\r\n  display: block;\r\n  white-space: nowrap;\r\n  overflow: hidden;\r\n  text-overflow: ellipsis;\r\n}\r\n\r\n.news-list ul li a:hover {\r\n  text-decoration: underline;\r\n}\r\n\r\n.news-list ul li .number {\r\n  font-weight: bold;\r\n  margin-right: 10px;\r\n  color: #555;\r\n  flex-shrink: 0;\r\n}\r\n\r\n.pagination {\r\n  text-align: center;\r\n  margin-top: 20px;\r\n}\r\n\r\n.pagination button {\r\n  padding: 10px 20px;\r\n  margin: 0 10px;\r\n  border: none;\r\n  background-color: #007bff;\r\n  color: #fff;\r\n  cursor: pointer;\r\n  border-radius: 5px;\r\n}\r\n\r\n.pagination button:disabled {\r\n  background-color: #ccc;\r\n  cursor: not-allowed;\r\n}\r\n</style>\r\n"],"mappings":"AAiBA;EACAA,KAAA;IACA;MACAC,UAAA;MACAC,YAAA;MACAC,QAAA;MACAC,QAAA;MACAC,WAAA;MACAC,UAAA;IACA;EACA;EACAC,QAAA;IACA,KAAAN,UAAA,QAAAO,MAAA,CAAAC,KAAA,CAAAR,UAAA;IACA,KAAAS,gBAAA;IACA,KAAAC,gBAAA;EACA;EACAC,OAAA;IACAF,iBAAA;MACA,KAAAG,QAAA,CAAAC,GAAA,yBAAAC,IAAA,CAAAC,GAAA;QACA,MAAAC,QAAA,GAAAD,GAAA,CAAAhB,IAAA,CAAAkB,IAAA,CAAAC,CAAA,IAAAA,CAAA,CAAAC,WAAA,SAAAnB,UAAA;QACA,IAAAgB,QAAA;UACA,KAAAf,YA